{
  "hosting": {
    "public": "build/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/webapp.ini",
        "destination": "/webapp.ini"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
